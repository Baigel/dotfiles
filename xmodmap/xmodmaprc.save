	# Useful Remaps
xmodmap -e "keycode 133 = Alt_L"
xmodmap -e "keycode 64 = Super_L" 
xmodmap -e "keycode 108 = Return"

# Easier Navigation
xmodmap -e "keycode 37 = Mode_switch
keysym j = j J Left
keysym k = k K Down
keysym l = l L Up"
